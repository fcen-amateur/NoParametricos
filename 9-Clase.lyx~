\begin_layout Standard
covarianza
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{E}\left[R\left(Y_{i}\right)R\left(Y_{j}\right)\right]-\mathbb{E}\left(R\left(Y_{i}\right)\right)\mathbb{E}\left(R\left(Y_{j}\right)\right)=\underset{s\neq k}{\sum_{s=0}^{N}\sum_{k=0}^{N}}sk\frac{1}{N\left(N-1\right)}-\left(\frac{N+1}{2}\right)^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{E}\left[R\left(Y_{i}\right)R\left(Y_{j}\right)\right]-\mathbb{E}\left(R\left(Y_{i}\right)\right)\mathbb{E}\left(R\left(Y_{j}\right)\right)=\frac{1}{N\left(N-1\right)}\underset{s\neq k}{\sum_{s=0}^{N}s\left(\sum_{k=0}^{N}k\right)}-\left(\frac{N+1}{2}\right)^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
sumo todos y resto el término donde 
\begin_inset Formula $s=k$
\end_inset

, 
\begin_inset Formula $s^{2}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{E}\left[R\left(Y_{i}\right)R\left(Y_{j}\right)\right]-\mathbb{E}\left(R\left(Y_{i}\right)\right)\mathbb{E}\left(R\left(Y_{j}\right)\right)=\frac{1}{N\left(N-1\right)}\sum_{s=0}^{N}\left[\sum_{k=0}^{N}sk-s^{2}\right]-\left(\frac{N+1}{2}\right)^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{E}\left[R\left(Y_{i}\right)R\left(Y_{j}\right)\right]-\mathbb{E}\left(R\left(Y_{i}\right)\right)\mathbb{E}\left(R\left(Y_{j}\right)\right)=\frac{1}{N\left(N-1\right)}\left[\sum_{s=0}^{N}\sum_{k=0}^{N}sk-\sum_{s=0}^{N}s^{2}\right]-\left(\frac{N+1}{2}\right)^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{E}\left[R\left(Y_{i}\right)R\left(Y_{j}\right)\right]-\mathbb{E}\left(R\left(Y_{i}\right)\right)\mathbb{E}\left(R\left(Y_{j}\right)\right)=\frac{1}{N\left(N-1\right)}\left[\sum_{s=0}^{N}s\sum_{k=0}^{N}k-\sum_{s=0}^{N}s^{2}\right]-\left(\frac{N+1}{2}\right)^{2}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mathbb{E}\left[R\left(Y_{i}\right)R\left(Y_{j}\right)\right]-\mathbb{E}\left(R\left(Y_{i}\right)\right)\mathbb{E}\left(R\left(Y_{j}\right)\right) & =\\
 & \frac{1}{N\left(N-1\right)}\left[\sum_{s=0}^{N}s\frac{N\left(N+1\right)}{2}-\frac{N\left(N+1\right)\left(2N+1\right)}{6}\right]-\left(\frac{N+1}{2}\right)^{2}
\end{align*}

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\begin{align*}
\mathbb{E}\left[R\left(Y_{i}\right)R\left(Y_{j}\right)\right]-\mathbb{E}\left(R\left(Y_{i}\right)\right)\mathbb{E}\left(R\left(Y_{j}\right)\right) & =\frac{N+1}{12}
\end{align*}

\end_inset


\end_layout

\begin_layout Corollary
Si 
\begin_inset Formula $U=\sum_{j=1}^{m}R\left(Y_{j}\right)$
\end_inset

 entonces 
\begin_inset Formula $\mathbb{E}\left(U\right)=m\left(\frac{N+1}{2}\right)$
\end_inset

, 
\begin_inset Formula $T=\sum_{i=1}^{n}R\left(X_{i}\right)$
\end_inset

 y 
\begin_inset Formula $\mathbb{E}\left(T\right)=n\frac{N+1}{2}$
\end_inset

, 
\begin_inset Formula $\mathbb{E}\left(W\right)=\mathbb{E}\left(U-\frac{m\left(m+1\right)}{2}\right)=m\left(\frac{N+1}{2}\right)-\frac{m\left(m+1\right)}{2}=\frac{mn}{2}$
\end_inset

.
\end_layout

\begin_layout Corollary
Caso de la varianza
\end_layout

\begin_layout Corollary
\begin_inset Formula 
\[
V\left(U\right)=V\left(\sum_{j=1}^{n}R\left(Y_{j}\right)\right)
\]

\end_inset


\end_layout

\begin_layout Corollary
Los rangos no son independientes, aparecen covarianzas.
\end_layout

\begin_layout Corollary
\begin_inset Formula 
\[
V\left(U\right)=\sum_{j=1}^{n}V\left(R\left(Y_{j}\right)\right)+\sum_{i\neq j}^{n}\text{Cov}\left[R\left(Y_{j}\right);R\left(Y_{i}\right)\right]
\]

\end_inset


\end_layout

\begin_layout Corollary
\begin_inset Formula 
\[
V\left(U\right)=m\frac{N^{2}-1}{12}+m\left(m-1\right)\frac{N+1}{12}
\]

\end_inset


\end_layout

\begin_layout Corollary
\begin_inset Formula 
\[
V\left(U\right)=\left(N-1+m-1\right)m\frac{N+1}{12}
\]

\end_inset


\end_layout

\begin_layout Corollary
\begin_inset Formula 
\[
V\left(U\right)=nm\frac{N+1}{12}
\]

\end_inset


\end_layout

\begin_layout Section
Convergencia en Probabilidad de 
\begin_inset Formula $\overline{W}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
H_{0}\Rightarrow\overline{W}=\frac{W}{nm}\overset{\mathcal{P}}{\rightarrow}\frac{1}{2}
\]

\end_inset


\end_layout

\begin_layout Proof
Usamos 
\begin_inset Formula $\mathbb{E}\left(X_{n}\right)\rightarrow\mu_{0}$
\end_inset

 y 
\begin_inset Formula $V\left(X_{n}\right)\rightarrow0\Rightarrow X_{n}\overset{p}{\rightarrow}\mu_{0}$
\end_inset

.
\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
\mathbb{E}\left(W\right)=\frac{nm}{2}\Rightarrow\mathbb{E}\left(\frac{W}{nm}\right)=\frac{1}{2}
\]

\end_inset


\end_layout

\begin_layout Proof
\begin_inset Formula 
\[
V\left(W\right)=\frac{1}{n^{2}m^{2}}V\left(W\right)=\frac{nm\left(N+1\right)}{n^{2}m^{2}}=\frac{1}{nm}\frac{N+1}{2}\Rightarrow V\left(\frac{W}{nm}\right)\rightarrow\frac{N}{2nm}+\frac{1}{2nm}\rightarrow0
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Separator plain
\end_inset


\end_layout

\begin_layout Section
Forma Recursiva de la Distribución Exacta de la Prueba de MWW
\end_layout

\begin_layout Standard
Dado un 
\begin_inset Formula $k$
\end_inset

 fijo:
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\Delta_{n;m}^{\left(k\right)}=\left\{ \text{número de secuencias de }x\text{ e }y\text{ para las cuales }W=k\right\} 
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P\left(W=k\right)=\frac{\Delta_{n;m}\left(k\right)}{\binom{m+n}{m}}
\]

\end_inset


\end_layout

\begin_layout Standard
Partámoslo en unión de conjuntos disjuntos
\end_layout

\begin_layout Standard
\begin_inset Formula $A_{1}:$
\end_inset

 secuencias donde 
\begin_inset Formula $X^{\left(n\right)}>Y^{\left(m\right)}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula $A_{2}$
\end_inset

: secuencias donde 
\begin_inset Formula $Y^{\left(m\right)}>X^{\left(n\right)}$
\end_inset

.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\left\{ A_{1}\cup A_{2}\right\} \cap\left\{ W=k\right\} =\Delta_{n;m}\left(k\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P\left(W=k\right)=\frac{\#\left(A_{1}\cap\left\{ W=k\right\} \right)}{\text{falta ver}}+\frac{\#\left(A_{2}\cap\left\{ W=k\right\} \right)}{\text{falta ver}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\#\left(A_{1}\cap\left\{ W=k\right\} \right)=\Delta_{n-1;m}\left(k\right)$
\end_inset

 dado que condicioné el último elemento, no aporta ningún 1 a la suma y
 no aporta información.
\end_layout

\begin_layout Standard
\begin_inset Formula $\#\left(A_{2}\cap\left\{ W=k\right\} \right)=\Delta_{n;m-1}\left(k-n\right)$
\end_inset

.
\end_layout

\begin_layout Standard
Como 
\begin_inset Formula $W=\sum_{ij}s\left(Y_{i}-X_{i}\right)$
\end_inset

.
 Al retirar el mayor elemento que era 
\begin_inset Formula $Y^{\left(m\right)}$
\end_inset

, perdemos 
\begin_inset Formula $n$
\end_inset

 resultados de signo positivo.
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
W=k=\sum_{ij}s\left(Y_{j}-X_{i}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
W=k=\sum_{i}\sum_{j}s\left(Y^{\left(j\right)}-X_{i}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
W=k=\sum_{i}\left[s\left(Y^{\left(j\right)}-X_{i}\right)+\sum_{j=1}^{m-1}s\left(Y^{\left(j\right)}-X_{i}\right)\right]
\]

\end_inset


\end_layout

\begin_layout Standard
Como estamos en 
\begin_inset Formula $A_{2}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
W=k=\sum_{i}\left[1+\sum_{j=1}^{m-1}s\left(Y^{\left(j\right)}-X_{i}\right)\right]
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
W=k=\sum_{i}1+\sum_{i}\sum_{j=1}^{m-1}sa\left(Y^{\left(j\right)}-X_{i}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
k=n+\sum_{i}\sum_{j=1}^{m-1}s\left(Y^{\left(j\right)}-X_{i}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
k-n=\sum_{i}\sum_{j=1}^{m-1}s\left(Y^{\left(j\right)}-X_{i}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
k-n=\underbrace{\sum_{i}\sum_{j=1}^{m-1}s\left(Y^{\left(j\right)}-X_{i}\right)}_{W/\Delta_{n;m-1}}
\]

\end_inset


\end_layout

\begin_layout Standard
Ahora queda
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\underbrace{P\left(W=k\right)}_{P_{n;m}\left(k\right)}=\frac{\Delta_{n-1;m}\left(k\right)}{\binom{n+m}{m}}+\frac{\Delta_{n;m-1}\left(k-n\right)}{\binom{n+m}{m}}
\]

\end_inset


\end_layout

\begin_layout Standard
buscamos vincunlar con el 
\begin_inset Formula $P$
\end_inset

 precedente
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\underbrace{P\left(W=k\right)}_{P_{n;m}\left(k\right)}=\frac{\Delta_{n-1;m}\left(k\right)}{\binom{m+n-1}{m}}\cdot\frac{\binom{m+n-1}{m}}{\binom{n+m}{m}}+\frac{\Delta_{n;m-1}\left(k-n\right)}{\binom{m+n-1}{m-1}}\cdot\frac{\binom{m+n-1}{m-1}}{\binom{n+m}{m}}
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\frac{\left(m+n-1\right)!}{n!\left(m-1\right)!}}{\frac{\left(m+n\right)!}{m!n!}}=\frac{m}{m+n}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula $\frac{\binom{m+n-1}{m-1}}{\binom{n+m}{m}}=\frac{n}{m+n}$
\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
P_{n;m}\left(k\right)=\frac{m}{m+n}\cdot P_{n-1;m}\left(k\right)+\frac{n}{m+n}\cdot P_{n;m-1}\left(k-n\right)
\]

\end_inset


\end_layout

\begin_layout Itemize
Nos faltan los casos de cierre.
\end_layout

\begin_layout Itemize
Buscarlos.
 
\begin_inset Formula $\Delta_{i;j}\left(k\right)=0$
\end_inset

 para 
\begin_inset Formula $k<0$
\end_inset

.
 
\begin_inset Formula $\Delta_{0;1}\left(k\right)=\begin{cases}
1 & k=0\\
0 & k\ne0
\end{cases}$
\end_inset

.
\end_layout

