\begin_layout Subsection
Estadístico de Mann y Whitney
\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
W=\sum_{ij}\left(Y_{i}-W_{j}\right)
\]

\end_inset


\end_layout

\begin_layout Standard
