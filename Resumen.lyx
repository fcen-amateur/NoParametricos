\end_inset


\end_layout

\begin_layout Definition
Estadístico generado por una FGS.
\end_layout

\begin_layout Definition
Sea 
\begin_inset Formula $a\left(i\right)=\Psi\left(\frac{i}{n+1}\right)$
\end_inset

.
\end_layout

\begin_layout Definition
\begin_inset Formula 
\[
\overline{V}=\frac{1}{n}\sum_{i=1}^{n}\Psi\left(\frac{R_{j}}{n+1}\right)s\left(X_{j}\right)=\frac{1}{n}\sum_{i=1}^{n}\Psi\left(\frac{j}{n+1}\right)w_{j}
\]

\end_inset


\end_layout

\begin_layout Theorem*
Esperanza y Varianza y distribución asintótica de un estadístico 
\begin_inset Formula $\overline{V}$
\end_inset

 generado por 
\begin_inset Formula $\Psi$
\end_inset

.
\end_layout

\begin_layout Theorem*
\begin_inset Formula 
\[
\mathbb{E}\left(\overline{V}\right)=\frac{1}{2n}\sum_{j=1}^{n}\Psi\left(\frac{j}{n+1}\right)\rightarrow\frac{1}{2}\int_{0}^{1}\Psi\left(u\right)du
\]

\end_inset


\end_layout

\begin_layout Theorem*
\begin_inset Formula 
\[
n\mathbb{V}\left(\overline{V}\right)=\frac{1}{4n}\sum_{j=1}^{n}\Psi^{2}\left(\frac{j}{n+1}\right)\rightarrow\frac{1}{4}\int_{0}^{1}\Psi^{2}\left(u\right)du
\]

\end_inset


\end_layout

\begin_layout Theorem*
\begin_inset Formula 
\[
\frac{\overline{V}-\mathbb{E}\left(\overline{V}\right)}{\sqrt{\text{Var}\left(\overline{V}\right)}}\rightarrow\mathcal{N}
\]

\end_inset


\end_layout

