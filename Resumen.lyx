\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\mathbb{V}\left(V_{1}\right)=\frac{1}{4}\sum_{j=1}^{n}a^{2}\left(j\right)
\]

\end_inset


\end_layout

\begin_layout Standard
\begin_inset Formula 
\[
\text{Cov}\left(V_{1};V_{2}\right)=\frac{1}{4}\sum_{j=1}^{n}a\left(j\right)b\left(j\right)
\]

\end_inset


\end_layout

